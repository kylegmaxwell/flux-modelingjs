{
  "name": "flux-modelingjs",
  "version": "0.0.6",
  "description": "Modeling entity constructors, measurement unit handlers, JSON schema validators etc.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "npm run all-tests",
    "all-tests": "npm run unit-tests && npm run integration-tests",
    "integration-tests": "npm run build-test && PATH=$(npm bin):$PATH jasmine-node --verbose --captureExceptions ./testsIntegration/",
    "unit-tests": "PATH=$(npm bin):$PATH jasmine-node --verbose --captureExceptions ./tests/",
    "build-test": "rollup -c rollup-test.config.js -f=cjs -o=./build/rollup-test.common.js;"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/vannevartech/flux-modelingjs.git"
  },
  "author": "Flux Factory",
  "license": "ISC",
  "homepage": "https://bitbucket.org/vannevartech/flux-modelingjs",
  "dependencies": {
    "ajv": "^3.8.10",
    "json-pointer": "^0.5.0"
  },
  "devDependencies": {
    "jasmine-node": "^1.14.1",
    "rollup": "^0.26.7",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollup-plugin-replace": "^1.1.0"
  }
}
